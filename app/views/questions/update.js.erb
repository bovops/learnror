<% if resource.errors.present? %>
$('form#edit_question_<%= resource.id %>').html('<%= j render 'form', submit: 'Update Question' %>');
<% else %>
$('.question').html('<%= j render 'question' %>');
$('#EditQuestionForm').modal('hide');
$('body').removeClass('modal-open');
$('.modal-backdrop').remove();
<% end %>
